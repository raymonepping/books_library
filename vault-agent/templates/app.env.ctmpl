# ./vault-agent/templates/app.env.ctmpl
# Renders KV v2 data at secret/data/app/backend
# Requires policy for: secret/data/app/backend

DB_USER={{ with secret "secret/data/app/backend" }}{{ .Data.data.DB_USER }}{{ end }}
DB_PASS={{ with secret "secret/data/app/backend" }}{{ .Data.data.DB_PASS }}{{ end }}
API_KEY={{ with secret "secret/data/app/backend" }}{{ .Data.data.API_KEY }}{{ end }}

